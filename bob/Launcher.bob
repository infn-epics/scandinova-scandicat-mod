    <horizontal_alignment>1</horizontal_alignment>
  </widget>
  <widget type="action_button" version="3.0.0">
    <name>MOD01_Button</name>
    <actions>
      <action type="open_display">
        <description>MOD01 - 1200V</description>
        <file>ScandinovaRFMod_Main.bob</file>
        <macros>
          <DEVICE>LEL:MOD:SCAT:MOD01</DEVICE>
        </macros>
        <target>tab</target>
      </action>
    </actions>
    <text>MOD01 (1200V)</text>
    <x>50</x>
    <y>50</y>
    <width>200</width>
    <height>30</height>
