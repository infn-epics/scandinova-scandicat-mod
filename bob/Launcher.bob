<?xml version="1.0" encoding="UTF-8"?>
<display version="2.0.0">
  <name>Scandinova Scandicat Modulator - Launcher</name>
  <width>300</width>
  <height>260</height>
  <widget type="label" version="2.0.0">
    <name>Title</name>
    <text>Scandinova Scandicat Modulators</text>
    <x>10</x>
    <y>10</y>
    <width>280</width>
    <height>30</height>
    <font>
